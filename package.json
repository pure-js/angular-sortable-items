{
  "name": "angular-sortable-items",
  "version": "1.0.0",
  "description": "angular application which allow you to sort lists of items",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080 --open",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "pree2e": "webdriver-manager update",
    "test": "karma start",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [
    "angular"
  ],
  "author": "pure-js",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.0.1",
    "@angular/common": "^5.0.1",
    "@angular/compiler": "^5.0.1",
    "@angular/compiler-cli": "^5.0.1",
    "@angular/core": "^5.0.1",
    "@angular/forms": "^5.0.1",
    "@angular/http": "^5.0.1",
    "@angular/platform-browser": "^5.0.1",
    "@angular/platform-browser-dynamic": "^5.0.1",
    "@angular/platform-server": "^5.0.1",
    "@angular/router": "^5.0.1",
    "angular-in-memory-web-api": "^0.5.1",
    "core-js": "^2.5.1",
    "octicons": "^7.0.1",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.3",
    "@types/node": "^8.0.50",
    "angular2-template-loader": "^0.6.2",
